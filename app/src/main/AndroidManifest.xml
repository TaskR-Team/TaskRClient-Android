        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".home.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            </activity>
        <activity android:name=".itemdetail.ItemDetailActivity">
        </activity>
        <activity android:name=".itemdetail.ItemDetailActivity"></activity>
        <activity android:name=".home.workitemviewmodel.AddWorkItemActivity"></activity>
        <activity android:name=".teamdetail.TeamDetailActivity"></activity>
    </application>

</manifest>